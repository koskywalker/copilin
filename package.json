{
  "name": "link-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "firebase": "5.9.0",
    "vue": "2.6.9",
    "vue-router": "3.0.2",
    "vuetify": "1.5.6",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "5.15.1",
    "@mdi/font": "3.5.95",
    "@vue/cli-plugin-babel": "3.5.1",
    "@vue/cli-plugin-eslint": "3.5.1",
    "@vue/cli-service": "3.5.1",
    "babel-eslint": "10.1.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.15.2",
    "eslint-plugin-vue": "5.2.2",
    "material-design-icons-iconfont": "4.0.5",
    "node-sass": "4.13.1",
    "sass-loader": "7.1.0",
    "vue-template-compiler": "2.6.9"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
